<style>


	.wishlist .button_Move {
		outline: none;
		padding: 9px 20px;
		background: #495057;
		color: #000000;
		border-radius: 3px;
		border: 2px solid;
		background: #ffffff;
		border-color: #7f7e7e;
		position: absolute;
		right: 29px;
		margin-top: -47px;
	}


	.wishlist-one img {
		width: 100%;
		padding: 20px;
		background-color: #F5F5F5;
		height: auto;
	}

	.wishlist-one {
    line-height: 13px;
    margin-top: 10px;
}

	.btn-primary {

		text-transform: capitalize;
	}


	.button_name button {
		color: white;
		font-size: 14px;
		font-weight: 700;
	}

	.button_name .btn.btn-primary {
		padding: 6px 0;
		width: 100%;
		background-color: #953A93;
	}


	.product_name {
		margin-top: 14px;
	}

	.product_name a {
		color: black;
		font-size: 14px;
		font-weight: 500;
		line-height:1.3;
	}

	.wishlist .price {
		color: #953A93;
		font-size: 16px;
		font-weight: 500;
	}

.wishlist .button_remove a {
    color: black;
    position: absolute;
    top: 32px;
    right: 20px;
    background-color: white;
    border-radius: 30px;
    padding: 10px;
    border: none;
}

	#content h2 {
		font-size: 20px;
		color: #000000;
		font-weight: 400;
	}


	.button_name .btn.btn-danger {
		padding: 9px 8px;
	}

	.continue .btn {
		outline: none;
		padding: 9px 20px;

		color: #fff;
		border-radius: 3px;
		border: none;
		background: #CB7FDE;
		border-color: #ffffff;
	}
</style>


<div id="account-wishlist" class="container">


	<ul class="breadcrumb">
		{% for breadcrumb in breadcrumbs %}
			<li>
				<a href="{{ breadcrumb.href }}">{{ breadcrumb.text }}</a>
			</li>
		{% endfor %}
	</ul>

	{% if success %}
		<div class="alert alert-success alert-dismissible">
			<i class="fa fa-check-circle"></i>
			{{ success }}
			<button type="button" class="close" data-dismiss="alert">&times;</button>
		</div>
	{% endif %}
	<div class="row" style='display:flex; gap:20px;'>
	 {#   {{ column_left }}#}
		{#{% if column_left and column_right %}#}
		{#	{% set class = 'col-sm-6' %}#}
		{#{% elseif column_left or column_right %}#}
		{#	{% set class = 'col-sm-9' %}#}
		{#{% else %}#}
		{#	{% set class = 'col-sm-12' %}#}
		{#{% endif %}#}
            {{column_left_account}}

		<div id="content" class="{{ class }}">{{ content_top }}


			{#<h2>{{ heading_title }}</h2>#}
			{% if products %}


				<div class="wishlist">

					<div class="container">

						{# <button type="button" class="btn button_Move">Move All To Bag
						</button> #}


						<div class="row">


							{% for product in products %}

								<div class="col-md-4">

									<div class="wishlist-one">


										<td class="text-center">
											{% if product.thumb %}
												<a href="{{ product.href }}"><img src="{{ product.thumb }}" alt="{{ product.name }}" title="{{ product.name }}"/></a>
											{% endif %}
										</td><br>

										<div class="button_name">
											<button type="button" onclick="cart.add('{{ product.product_id }}');" data-toggle="tooltip" title="{{ button_cart }}" class="btn btn-primary">
												<i class="fa fa-shopping-cart"></i>Add To Cart
											</button>
										</div>
										<br>


										<div class="product_name">
											<a href="{{ product.href }}">{{ product.name }}</a>
										</div><br>

										<td class="text-center">
											{% if product.price %}
												<div class="price">
													{% if not product.special %}
														{{ product.price }}
													{% else %}
														<b>{{ product.special }}</b>
														<s style='color:gray; margin-left:5px;'>{{ product.price }}</s>
													{% endif %}
												</div>
											{% endif %}
										</td>

										<div class="button_remove">

											<a href="{{ product.remove }}" data-toggle="tooltip" title="{{ button_remove }}" class="btn btn-danger">
												<i class="fa fa-trash-o" aria-hidden="true"></i>
											</a>

										</div>


									</div>

								</div>

							{% endfor %}


						</div>


					</div>


				</div>


			{% else %}
				<p>{{ text_empty }}</p>
			{% endif %}
			<div class="buttons clearfix">
				{# <div class="pull-right continue">
					<a href="{{ continue }}" class="btn">{{ button_continue }}</a>
				</div> #}
			</div>
			{{ content_bottom }}</div>
		{{ column_right }}</div>


</div>
{{ footer }}
                                  <td>{{ ok }}</td>
                                {% elseif category_feeds[store.store_id][lang.code][currency.code|lower].status == -1 %}
                                  <td>{{ err }}</td>
                                {% else %}
                                  <td>{{ category_feeds[store.store_id][lang.code][currency.code|lower].status }}</td>
                                {% endif %} 
                                <td>{{ category_feeds[store.store_id][lang.code][currency.code|lower].last_change }}</td>
                                <td valign="middle">
                                  <div class="progress">
                                    <div class="progress-bar progress-bar-success" role="progressbar" aria-valuenow="{{ category_feeds[store.store_id][lang.code][currency.code|lower].progress }}" aria-valuemin="0" aria-valuemax="100" style="min-width: {% if category_feeds[store.store_id][lang.code][currency.code|lower].progress == 0 %} 2em {% else %} {{ category_feeds[store.store_id][lang.code][currency.code|lower].progress }} {% endif %}; width: {{ category_feeds[store.store_id][lang.code][currency.code|lower].progress }}%;">
                                      {{ category_feeds[store.store_id][lang.code][currency.code|lower].progress }}%
                                    </div>
                                  </div>
                                </td>
                                <td align="left">                               
                                  <div class="input-group">
                                    <input type="url" class="form-control" value="{{ category_feeds[store.store_id][lang.code][currency.code|lower].path }}" readonly >
                                    <div class="input-group-btn">
                                      <a href="javascript:void(0);" data-clipboard-text="{{ category_feeds[store.store_id][lang.code][currency.code|lower].path }}" class="btn btn-primary btn-copy" data-toggle="tooltip" data-placement="top" title="{{ copy_feed_btn }}">
                                        <i class="fa fa-clipboard"></i>
                                      </a>
                                      <a href="javascript:void(0);" onclick="exportFeed(this)" data-url="{{ stores[0].url }}index.php?route={{ route }}&a=cf&s={{ store.store_id }}&l={{ lang.code }}&c={{ currency.code|lower }}&h={{ category_feeds[store.store_id][lang.code][currency.code|lower].cron_hash }}"  class="btn btn-success" data-toggle="tooltip" data-placement="top" title="{{ run_cron_btn }}">
                                        <i class="fa fa-play"></i>
                                      </a>
                                      {% if category_feeds[store.store_id][lang.code][currency.code|lower].exists == "1" %}
                                      <a href="{{ category_feeds[store.store_id][lang.code][currency.code|lower].path }}" class="btn btn-warning" target="_blank" data-toggle="tooltip" data-placement="top" title="{{ open_feed_btn }}">
                                        <i class="fa fa-link"></i>
                                      </a>
                                      <a href="javascript:void(0);" onclick="deleteFeedDialog(this)" data-abs-path="{{ category_feeds[store.store_id][lang.code][currency.code|lower].abs_path }}" data-path="{{ category_feeds[store.store_id][lang.code][currency.code|lower].path }}" data-hash="{{ category_feeds[store.store_id][lang.code][currency.code|lower].hash }}" class="btn btn-danger btn-delete" data-toggle="tooltip" data-placement="top" title="{{ delete_feed_btn }}">
                                        <i class="fa fa-trash"></i>
                                      </a>
                                      {% else %}
                                      <a href="javascript:void(0);" class="btn btn-warning disabled" data-toggle="tooltip" data-placement="top" title="{{ open_feed_btn }}">
                                        <i class="fa fa-link"></i>
                                      </a>
                                      <a href="javascript:void(0);" class="btn btn-danger disabled" data-toggle="tooltip" data-placement="top" title="{{ delete_feed_btn }}">
                                        <i class="fa fa-trash"></i>
                                      </a>
                                      {% endif %}
                                    </div>
                                  </div>
                                </td>      
                              </tr>
                            {% endfor %}
                          {% endfor %}
                          </table>

                        </div>
                      </div>
                      <!-- ./category feeds -->

                      <!-- heureka availability feeds -->
                      <div class="panel panel-default">
                        <div class="panel-heading"><strong>{{ heureka_availability_feeds_heading }}</strong></div>
                        <div class="panel-body">

                          <table class="table table-striped table-bordered">
                            <tr>
                              <th style="width: 120px;"></th>
                              <th><strong>{{ feed_last_creation }}</strong></th>
                              <th><strong>{{ feed_status }}</strong></th>
                              <th><strong>{{ feed_last_change }}</strong></th>
                              <th><strong>{{ feed_progress }}</strong></th>
                              <th><strong>{{ feed_url }}</strong></th>
                            </tr>
                              <tr>
                                <td></td>
                                <td>{{ heureka_availability_feeds[store.store_id].last_creation }}</td>
                                {% if heureka_availability_feeds[store.store_id].status == 1 %}
                                  <td>{{ ok }}</td>
                                {% elseif heureka_availability_feeds[store.store_id].status == -1 %}
                                  <td>{{ err }}</td>
                                {% else %}
                                  <td>{{ heureka_availability_feeds[store.store_id].status }}</td>
                                {% endif %} 
                                <td>{{ heureka_availability_feeds[store.store_id].last_change }}</td>
                                <td valign="middle">
                                  <div class="progress">
                                    <div class="progress-bar progress-bar-success" role="progressbar" aria-valuenow="{{ heureka_availability_feeds[store.store_id].progress }}" aria-valuemin="0" aria-valuemax="100" style="min-width: {% if heureka_availability_feeds[store.store_id].progress == 0 %} 2em {% else %} {{ heureka_availability_feeds[store.store_id].progress }} {% endif %}; width: {{ heureka_availability_feeds[store.store_id].progress }}%;">
                                      {{ heureka_availability_feeds[store.store_id].progress }}%
                                    </div>
                                  </div>
                                </td>
                                <td align="left">                               
                                  <div class="input-group">
                                    <input type="url" class="form-control" value="{{ heureka_availability_feeds[store.store_id].path }}" readonly >
                                    <div class="input-group-btn">
                                      <a href="javascript:void(0);" data-clipboard-text="{{ heureka_availability_feeds[store.store_id].path }}" class="btn btn-primary btn-copy" data-toggle="tooltip" data-placement="top" title="{{ copy_feed_btn }}">
                                        <i class="fa fa-clipboard"></i>
                                      </a>
                                      <a href="javascript:void(0);" onclick="exportFeed(this)" data-url="{{ stores[0].url }}index.php?route={{ route }}&a=haf&s={{ store.store_id }}&h={{ heureka_availability_feeds[store.store_id].cron_hash }}"  class="btn btn-success" data-toggle="tooltip" data-placement="top" title="{{ run_cron_btn }}">
                                        <i class="fa fa-play"></i>
                                      </a>
                                      {% if heureka_availability_feeds[store.store_id].exists == "1" %}
                                      <a href="{{ heureka_availability_feeds[store.store_id].path }}" class="btn btn-warning" target="_blank" data-toggle="tooltip" data-placement="top" title="{{ open_feed_btn }}">
                                        <i class="fa fa-link"></i>&nbsp;
                                      </a>
                                      <a href="javascript:void(0);" onclick="deleteFeedDialog(this)" data-abs-path="{{ heureka_availability_feeds[store.store_id].abs_path }}" data-path="{{ heureka_availability_feeds[store.store_id].path }}" data-hash="{{ heureka_availability_feeds[store.store_id].hash }}" class="btn btn-danger btn-delete" data-toggle="tooltip" data-placement="top" title="{{ delete_feed_btn }}">
                                        <i class="fa fa-trash"></i>
                                      </a>
                                      {% else %}
                                      <a href="javascript:void(0);" class="btn btn-warning disabled" data-toggle="tooltip" data-placement="top" title="{{ open_feed_btn }}">
                                        <i class="fa fa-link"></i>
                                      </a>
                                      <a href="javascript:void(0);" class="btn btn-danger disabled" data-toggle="tooltip" data-placement="top" title="{{ delete_feed_btn }}">
                                        <i class="fa fa-trash"></i>
                                      </a>
                                      {% endif %}

                                    </div>
                                  </div>
                                </td>      
                              </tr>
                          </table>

                        </div>
                      </div>
                      <!-- ./heureka availability feeds -->

                    </div>
                    {% endfor %}
                </div>
            </div>
            <!-- ./stores -->

        </div>
    </div>
    <div class="row">
      <!-- bottom banner -->
      {{ bottom_banner_view }}
      <!-- ./bottom banner -->
    </div>
</div> 
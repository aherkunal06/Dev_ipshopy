<div class="tab-pane" id="feeds">
    <div class="row">
        <div class="col-sm-12 col-md-12">


            <div class="alert alert-info" role="alert"><span class="icon fa fa-info-circle"></span>&nbsp;{{ feeds_text }}</div>

            <!-- stores -->
            <div>
                <ul class="nav nav-tabs" id="xml-store-tabs">
                {% for store in stores %}
                    <li><a href="#xml-store-{{ store.store_id }}" data-toggle="tab"><i class="fa fa-shopping-cart"></i>&nbsp;{{ store.name }}</a></li>
                {% endfor %}
                </ul>

                <!-- Tab panes -->
                <div class="tab-content">
                    {% for store in stores %}
                    <div class="tab-pane" id="xml-store-{{ store.store_id }}">
                    
                      <!-- product feeds -->
                      <div class="panel panel-default">
                        <div class="panel-heading"><strong>{{ product_feeds_heading }}</strong></div>
                        <div class="panel-body">

                          <table class="table table-striped table-bordered">
                            <tr>
                              <th style="width: 120px;"><strong>{{ feed_lang }}</strong></th>
                              <th><strong>{{ feed_last_creation }}</strong></th>
                              <th><strong>{{ feed_status }}</strong></th>
                              <th><strong>{{ feed_last_change }}</strong></th>
                              <th><strong>{{ feed_progress }}</strong></th>
                              <th><strong>{{ feed_url }}</strong></th>
                            </tr>
                            {% for lang in languages %}
                              {% for currency in currencies %}
                              <tr>
                                <td>{{ lang.name ~ " - " ~ currency.code }}</td>
                                <td>{{ product_feeds[store.store_id][lang.code][currency.code|lower].last_creation }}</td>
                                {% if product_feeds[store.store_id][lang.code][currency.code|lower].status == 1 %}
                                  <td>{{ ok }}</td>
                                {% elseif product_feeds[store.store_id][lang.code][currency.code|lower].status == -1 %}
                                  <td>{{ err }}</td>
                                {% else %}
                                  <td>{{ product_feeds[store.store_id][lang.code][currency.code|lower].status }}</td>
                                {% endif %}
                                <td>{{ product_feeds[store.store_id][lang.code][currency.code|lower].last_change }}</td>
                                <td valign="middle">
                                  <div class="progress">
                                    <div class="progress-bar progress-bar-success" role="progressbar" aria-valuenow="{{ product_feeds[store.store_id][lang.code][currency.code|lower].progress }}" aria-valuemin="0" aria-valuemax="100" style="min-width: {% if product_feeds[store.store_id][lang.code][currency.code|lower].progress == 0 %} 2em {% else %} {{ product_feeds[store.store_id][lang.code][currency.code|lower].progress }} {% endif %};width: {{ product_feeds[store.store_id][lang.code][currency.code|lower].progress }}%;" style="min-width: 2em;">
                                      {{ product_feeds[store.store_id][lang.code][currency.code|lower].progress }}%
                                    </div>
                                  </div>
                                </td>
                                <td align="left">
                                  <div class="input-group">
                                    <input type="url" class="form-control" value="{{ product_feeds[store.store_id][lang.code][currency.code|lower].path }}" readonly >
                                    <div class="input-group-btn">
                                      <a href="javascript:void(0);" data-clipboard-text="{{ product_feeds[store.store_id][lang.code][currency.code|lower].path }}" class="btn btn-primary btn-copy" data-toggle="tooltip" data-placement="top" title="{{ copy_feed_btn }}">
                                        <i class="fa fa-clipboard"></i>
                                      </a>
                                      <a href="javascript:void(0);" onclick="exportFeed(this)" data-url="{{ stores[0].url }}index.php?route={{ route }}&a=pf&s={{ store.store_id }}&l={{ lang.code }}&c={{ currency.code|lower }}&h={{ product_feeds[store.store_id][lang.code][currency.code|lower].cron_hash }}"  class="btn btn-success" data-toggle="tooltip" data-placement="top" title="{{ run_cron_btn }}">
                                        <i class="fa fa-play"></i>
                                      </a>
                                      {% if product_feeds[store.store_id][lang.code][currency.code|lower].exists == "1" %}
                                      <a href="{{ product_feeds[store.store_id][lang.code][currency.code|lower].path }}" class="btn btn-warning" target="_blank" data-toggle="tooltip" data-placement="top" title="{{ open_feed_btn }}">
                                        <i class="fa fa-link"></i>
                                      </a>
                                      <a href="javascript:void(0);" onclick="deleteFeedDialog(this)" data-abs-path="{{ product_feeds[store.store_id][lang.code][currency.code|lower].abs_path }}" data-path="{{ product_feeds[store.store_id][lang.code][currency.code|lower].path }}" data-hash="{{ product_feeds[store.store_id][lang.code][currency.code|lower].hash }}" class="btn btn-danger btn-delete" data-toggle="tooltip" data-placement="top" title="{{ delete_feed_btn }}">
                                        <i class="fa fa-trash"></i>
                                      </a>
                                      {% else %}
                                      <a href="javascript:void(0);" class="btn btn-warning disabled" data-toggle="tooltip" data-placement="top" title="{{ open_feed_btn }}">
                                        <i class="fa fa-link"></i>
                                      </a>
                                      <a href="javascript:void(0);" class="btn btn-danger disabled" data-toggle="tooltip" data-placement="top" title="{{ delete_feed_btn }}">
                                        <i class="fa fa-trash"></i>
                                      </a>
                                      {% endif %}
                                    </div>  
                                  </div>
                                </td>
                              </tr>
                              {% endfor %}
                            {% endfor %}
                          </table>

                        </div>
                      </div>
                      <!-- ./product feeds -->

                      <!-- category feeds -->

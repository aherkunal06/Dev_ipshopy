								</thead>
								<tbody>
									{% set image_row = 0 %}
									{% for tmdaccounins in tmdaccounticons %}
									<tr id="image-row{{ image_row }}">
										<td class="text-left"><div class="input-group"><input type="text" name="tmdaccount_icon[{{ image_row }}][icon]" value="{{ tmdaccounins.icon }}" placeholder="{{ entry_icon }}" class="form-control fontbox{{ image_row }}"><span class="input-group-addon"></span></div>
										</td>

										<td class="text-left">
